@{
    ViewData["Title"] = "Home";
}

        <h1 class="mb-5 text-center">Imperator Rome Stats</h1>
        <div class="row">
            <div class="col-md-6 offset-md-3">
                @* ReSharper disable once UnknownCssClass *@
                <div class="w-file-form">
                    <div class="form-group">
                        <form method="post" enctype="multipart/form-data" asp-controller="Home" asp-action="UploadSave">
                            <div class="custom-file">
                                <input type="file" name="files" class="custom-file-input" multiple />
                                <label class="custom-file-label" for="customFile"></label>
                            </div>
                            <div class="text-center">
                                <input type="submit" value="Upload" class="btn btn-imperator mt-3" id="customFile" />
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
